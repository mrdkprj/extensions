{

  "manifest_version": 3,
  "name": "ExportTabUrls",
  "version": "1.0",

  "description": "ExportTabUrls",

  "permissions": [
    "background",
    "tabs",
    "downloads",
    "scripting"
  ],

  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ],
      "all_frames": true,
      "run_at": "document_start"
    }
  ],

  "action": {
    "default_title": "Export"
  },

  "background": {
    "service_worker": "background.js"
  }

}
