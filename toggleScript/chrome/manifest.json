{

  "manifest_version": 3,
  "name": "ToggleScript",
  "version": "1.0",

  "description": "ToggleScript",

  "permissions": [
    "background",
    "declarativeNetRequest",
    "tabs",
    "storage"
  ],

  "icons": {
    "16": "./icons/enable/16.png",
    "32": "./icons/enable/32.png",
    "48": "./icons/enable/48.png",
    "128": "./icons/enable/128.png"
  },

  "host_permissions": [
    "<all_urls>"
  ],

  "action": {
    "default_title": "Enabled"
  },

  "background": {
    "service_worker": "background.js"
  }

}
